.ir
  background-color: transparent
  border: 0
  overflow: hidden
  /* IE 6/7 fallback */
  *text-indent: -9999px

.ir:before
  content: ""
  display: block
  width: 0
  height: 150%

.clearfix:after,
.clearfix:before
  content: " "
  display: table

.clearfix:after
  clear: both

.hide
  display: none
  visibility: hidden
